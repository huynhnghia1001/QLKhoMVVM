<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:QLKHODevExpress.ViewModels"
                    xmlns:view="clr-namespace:QLKHODevExpress.Views">
    <DataTemplate DataType="{x:Type vm:CustomerViewModel}">
        <view:Customer/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UnitViewModel}">
        <view:Unit/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ObjectViewModel}">
        <view:Object/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:SuplierViewModel}">
        <view:Suplier/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UserViewModel}">
        <view:User/>
    </DataTemplate>
    
</ResourceDictionary>